<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.campuslife">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo5"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo5"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Dashboard"></activity>
        <activity android:name=".PageSignUp" />
        <activity android:name=".PageLogin" />
        <activity android:name=".Homepage" />
        <activity android:name=".AddEvent"/>
        <activity android:name=".editProfile"/>
        <activity android:name=".changePassword"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>